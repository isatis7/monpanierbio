<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>MonPanierBio</display-name>
	<welcome-file-list>
		
		<welcome-file>index.jsp</welcome-file>
		
	</welcome-file-list>
	
<servlet>
	<servlet-name>WelcomeServlet</servlet-name>
	<servlet-class>com.monPanierBio.WelcomeServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>WelcomeServlet</servlet-name>
	<url-pattern>/welcome</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>SubmitFormServlet</servlet-name>
	<servlet-class>com.monPanierBio.SubmitFormServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>SubmitFormServlet</servlet-name>
	<url-pattern>/submit</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>PanierServlet</servlet-name>
	<servlet-class>com.monPanierBio.PanierServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>PanierServlet</servlet-name>
	<url-pattern>/panier</url-pattern>
</servlet-mapping>

<servlet>
	<servlet-name>RetourServlet</servlet-name>
	<servlet-class>com.monPanierBio.RetourServlet</servlet-class>
</servlet>

<servlet-mapping>
	<servlet-name>RetourServlet</servlet-name>
	<url-pattern>/retour</url-pattern>
</servlet-mapping>

</web-app>

